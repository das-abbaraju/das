<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.devMode" value="true" />
	<constant name="struts.url.includeParams" value="none" />
	<package name="com.picsauditing" extends="struts-default">
		<!-- exception section... I wrote this once, no sense wishing I had it later 
			<global-results>
			<result name="Exception">/sports/exception.jsp</result>
			</global-results>
			<global-exception-mappings>
			<exception-mapping exception="java.lang.Exception" result="Exception"/>
			</global-exception-mappings>
		-->

		<action name="Home"
			class="com.picsauditing.actions.Home">
			<result>struts/home.jsp</result>
		</action>
		
		<action name="RulesList"
			class="com.picsauditing.actions.RulesAction">
			<result>rules.jsp</result>
		</action>

		<action name="UsersManage"
			class="com.picsauditing.actions.users.UsersManage">
			<result>struts/users/users_manage.jsp</result>
		</action>

		<action name="ReportAuditList"
			class="com.picsauditing.actions.report.ReportContractorAudits">
			<result>struts/reports/auditList.jsp</result>
		</action>
		
		<action name="AuditListAuditor"
			class="com.picsauditing.actions.report.ReportContractorAuditAuditor">
			<result>struts/reports/audit_auditor.jsp</result>
		</action>
		
		<action name="AuditAssignments"
			class="com.picsauditing.actions.report.ReportContractorAuditAssignment">
			<result>struts/reports/auditAssignments.jsp</result>
		</action>

		<action name="AuditAssignmentUpdateAjax"
			class="com.picsauditing.actions.report.AuditAssignmentUpdate">
			<result>struts/blank.jsp</result>
		</action>

		<action name="FacilitiesGetAjax"
			class="com.picsauditing.actions.operators.FacilitiesGet">
			<result>struts/operators/facilitySelect.jsp</result>
		</action>

		<action name="AuditOperator"
			class="com.picsauditing.actions.audits.AuditOperatorList">
			<result>struts/operators/audit_operator.jsp</result>
		</action>
		<action name="AuditOperatorSaveAjax"
			class="com.picsauditing.actions.audits.AuditOperatorSave">
			<result>struts/operators/audit_operator_save.jsp</result>
		</action>
		
		<action name="VerifyView"
			class="com.picsauditing.actions.audits.VerifyView">
			<result>struts/contractors/pqf_verification_detail.jsp</result>
		</action>
		<action name="VerifySaveFollowUpAjax" method="saveFollowUp"
			class="com.picsauditing.actions.audits.VerifyView">
			<result>struts/blank.jsp</result>
		</action>
		<action name="VerifySwitchFilesAjax" method="switchFiles"
			class="com.picsauditing.actions.contractors.SwitchFiles">
			<result>struts/blank.jsp</result>
		</action>
		<action name="VerifySendEmailAjax" method="sendEmail"
			class="com.picsauditing.actions.audits.VerifyView">
			<result>struts/blank.jsp</result>
		</action>

		<action name="ContractorView"
			class="com.picsauditing.actions.contractors.ContractorView">
			<result>struts/contractors/con_view.jsp</result>
		</action>

		<action name="ConAuditList"
			class="com.picsauditing.actions.contractors.ConAuditList">
			<result>struts/contractors/contractor_audits.jsp</result>
		</action>

		<action name="AuditTypeChoose"
			class="com.picsauditing.actions.auditType.AuditTypeChoose">
			<result>struts/audits/audit_selectType.jsp</result>
		</action>

		<action name="ReportNCMS"
			class="com.picsauditing.actions.report.ReportAccountNCMS">
			<result>struts/reports/report_ncms.jsp</result>
		</action>

		<action name="AuditSaveAjax"
			class="com.picsauditing.actions.audits.AuditSave">
			<result>struts/blank.jsp</result>
		</action>

		<action name="ContractorSearch"
			class="com.picsauditing.actions.report.ReportAccount">
			<result>struts/reports/contractors_search.jsp</result>
		</action>

		<action name="ContractorListAdmin"
			class="com.picsauditing.actions.report.ReportAccountAudits">
			<result>struts/reports/contractors_admin.jsp</result>
		</action>
		
		<action name="ContractorListOperator"
			class="com.picsauditing.actions.report.ReportAccountAudits">
			<result>struts/reports/contractors_operator.jsp</result>
		</action>
	
		<action name="ProfileEdit"
			class="com.picsauditing.actions.users.ProfileEdit">
			<result>struts/users/profile_edit.jsp</result>
		</action>

		<action name="ContractorOperatorLimited"
			class="com.picsauditing.actions.report.ReportAccountAudits">
			<result>struts/reports/contractors_operator_limited.jsp</result>
		</action>
		
		
		<action name="ChartXMLFlagCount"
			class="com.picsauditing.actions.chart.ChartFlagCount">
			<result>struts/chart_xml.jsp</result>
		</action>

		<action name="ChartXMLTradeCount"
			class="com.picsauditing.actions.chart.ChartTradeCount">
			<result>struts/chart_xml.jsp</result>
		</action>

		<action name="ContractorRegistrationAjax"
			class="com.picsauditing.actions.contractors.ContractorsWidget">
			<result>struts/widgets/new_contractors.jsp</result>
		</action>
		
		<action name="UpcomingAuditsAjax"
			class="com.picsauditing.actions.audits.ContractorAuditsWidget">
			<result>struts/widgets/audits_upcoming.jsp</result>
		</action>
		
		<action name="ClosedAuditsAjax"
			class="com.picsauditing.actions.audits.ContractorAuditsWidget">
			<result>struts/widgets/audits_closed.jsp</result>
		</action>
		
		<action name="AuditorsGet"
			class="com.picsauditing.actions.audits.AuditorsGet">
			<result>/struts/audits/auditorSelect.jsp</result>
		</action>
		
	</package>
</struts>
