package com.picsauditing.rbic
dialect "java"

import com.picsauditing.rbic.ContractorModel;
import com.picsauditing.jpa.entities.LowMedHigh;

rule "numberOfSeatsInAircraft"
    when
        $con : ContractorModel($numberOfSeats : findQuestionAnswerAsInt(-3), $numberOfSeats > 0)
    then
        $con.updateCriteriaLimitForContractor(-2, -1,$numberOfSeats * 3000000);
    end
