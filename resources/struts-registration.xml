<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="com.picsauditing.registration" extends="com.picsauditing">
		<action name="Registration" class="com.picsauditing.actions.contractors.Registration">
			<result>struts/contractors/registration.jsp</result>
			<result name="input">struts/contractors/registration.jsp</result>
			<result name="blank">/struts/blank.jsp</result>
		</action>
		
		<action name="ContractorRegistration" class="com.picsauditing.actions.contractors.Registration">
			<result type="redirectAction">
				<param name="actionName">Registration</param>
				<param name="requestID">${requestID}</param>
				<param name="button">${button}</param>
			</result>
			<result name="input" type="redirectAction">
				<param name="actionName">Registration</param>
				<param name="requestID">${requestID}</param>
				<param name="button">${button}</param>
			</result>
			<result name="blank" type="redirectAction">
				<param name="actionName">Registration</param>
				<param name="requestID">${requestID}</param>
				<param name="button">${button}</param>
			</result>
		</action>
		
		<action name="RegistrationAddClientSite"
			class="com.picsauditing.actions.contractors.RegistrationAddClientSite">
			<result>struts/contractors/registrationAddClientSite.jsp</result>
			<result name="input">struts/contractors/registrationAddClientSite.jsp</result>
			<result name="ClientSiteList">struts/contractors/registrationAddClientSiteListAjax.jsp</result>
		</action>
		
		<action name="RegistrationServiceEvaluation"
			class="com.picsauditing.actions.contractors.RegistrationServiceEvaluation">
			<result>struts/contractors/registrationServiceEvaluation.jsp</result>
			<result name="input">struts/contractors/registrationServiceEvaluation.jsp</result>
		</action>

		<action name="RegistrationMakePayment"
			class="com.picsauditing.actions.contractors.RegistrationMakePayment">
			<result>struts/contractors/registrationMakePayment.jsp</result>
			<result name="print">struts/contractors/registrationMakePaymentPrint.jsp</result>
			<result name="blank">/struts/blank.jsp</result>
		</action>
	</package>
</struts>