<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
  	<!-- We're using MySQL database so the dialect needs to MySQL as well -->
		<property name="hibernate.dialect">org.hibernate.dialect.MySQL5InnoDBDialect</property>
		<!-- Enable this to see the SQL statements in the logs -->
		<property name="show_sql">true</property>

		<mapping class="com.klark.user.model.User" />
		<mapping class="com.klark.user.model.Beneficiary" />
		<mapping class="com.klark.user.model.UserMessage" />
		<mapping class="com.klark.user.model.UserMessageBeneficiary" />
		<mapping class="com.klark.user.model.ScheduleorUnscheduleMessage" />
		<mapping class="com.klark.user.model.ReceivedMessage" />



	</session-factory>
</hibernate-configuration>
