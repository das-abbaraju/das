<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:mockito="http://www.mockito.org/spring/mockito"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd


    http://www.mockito.org/spring/mockito https://bitbucket.org/kubek2k/springockito/raw/tip/springockito/src/main/resources/spring/mockito.xsd"
       default-autowire="autodetect">

    <bean id="SpringUtils" class="com.picsauditing.util.SpringUtils"/>

    <mockito:mock id="ContractorAuditDAO" class="com.picsauditing.dao.ContractorAuditDAO"/>
    <mockito:mock id="OperatorTagDAO" class="com.picsauditing.dao.OperatorTagDAO"/>
    <mockito:mock id="FeatureToggle" class="com.picsauditing.toggle.FeatureToggle"/>
    <mockito:mock id="LanguageModel" class="com.picsauditing.model.i18n.LanguageModel"/>

</beans>
