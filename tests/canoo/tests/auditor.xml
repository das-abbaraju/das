<?xml version="1.0"?>
<!DOCTYPE project SYSTEM "../dtd/Project.dtd">

<project name="PICS" default="test">
	<property name="login.username" value="ship" />
	<target name="test">
		<webtest name="Auditor">
			&config;
			<steps>
				&login;
				&body_verify;
				
				<setSelectField description="Choose Desktop"
					name="auditType" value="Desktop" />
				<clickButton description="Show Desktop Audits"
				            name="imageField"/>
				&body_verify;
				<verifyText description="Make sure there are multiple contractors found"
					text="You have .strong.[1-9][0-9]*..strong. contractors to audit" regex="true" />

				<clickLink description="Open Calendar" label="Audit Calendar"/>
				&body_verify;
			</steps>
		</webtest>
	</target>
</project>
