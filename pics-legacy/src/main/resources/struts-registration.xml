<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="com.picsauditing.registration" extends="com.picsauditing">
		<action name="Registration" class="com.picsauditing.struts.controller.Registration">
			<result>struts/contractors/registration.jsp</result>
			<result name="input">struts/contractors/registration.jsp</result>
			<result name="inputError">struts/contractors/registration.jsp</result>
			<result name="blank">/struts/blank.jsp</result>
            <result name="defaultAddressFields">/struts/contractors/_registration-company-address.jsp</result>
            <result name="GBAddressFields">/struts/contractors/united-kingdom/_registration-company-address.jsp</result>
            <result name="AUAddressFields">/struts/contractors/australia/_registration-company-address.jsp</result>
		</action>
		
		<action name="Registration2" class="com.picsauditing.struts.controller.AngularLoaderAction" method="load">
        </action>

        <action name="registration-requests/{registrationRequestKey}" class="com.picsauditing.struts.controller.Registration2" method="registrationFormJson">
        </action>

        <action name="accounts" class="com.picsauditing.struts.controller.Registration2" method="createAccount">
        </action>

        <action name="registration/validation" class="com.picsauditing.struts.controller.Registration2" method="validateInputs">
        </action>

        <action name="languages" class="com.picsauditing.struts.controller.Registration2" method="languages">
            <result name="forward" type="chain">
                <param name="actionName">languages</param>
                <param name="namespace">/employee-guard/api</param>
                <param name="method">index</param>
            </result>
        </action>

        <action name="dialects/{language}" class="com.picsauditing.struts.controller.Registration2"
                method="countriesBasedOn">
            <result name="forward" type="chain">
                <param name="actionName">dialects/{language}</param>
                <param name="namespace">/employee-guard/api</param>
                <param name="method">index</param>
            </result>
        </action>

        <action name="countries" class="com.picsauditing.struts.controller.Registration2" method="countries">
        </action>

        <action name="sales-phone/{isoCode}" class="com.picsauditing.struts.controller.Registration2" method="salesPhoneNumber">
        </action>

        <action name="locale" class="com.picsauditing.struts.controller.Registration2" method="locale">
        </action>

        <action name="mibew-base-url/{language}" class="com.picsauditing.struts.controller.Registration2" method="mibewBaseUrl">
        </action>

        <action name="RegistrationAjax" class="com.picsauditing.struts.controller.Registration" method="dialects">
            <result>struts/contractors/_registration-dialects.jsp</result>
            <result name="input">struts/contractors/_registration-dialects.jsp</result>
        </action>

		<action name="ContractorRegistration" class="com.picsauditing.struts.controller.Registration">
			<result type="redirectAction">
				<param name="actionName">Registration</param>
				<param name="requestID">${requestID}</param>
				<param name="button">${button}</param>
			</result>
			<result name="input" type="redirectAction">
				<param name="actionName">Registration</param>
				<param name="requestID">${requestID}</param>
				<param name="button">${button}</param>
			</result>
			<result name="blank" type="redirectAction">
				<param name="actionName">Registration</param>
				<param name="requestID">${requestID}</param>
				<param name="button">${button}</param>
			</result>
		</action>
		
		<action name="RegistrationAddClientSite"
			class="com.picsauditing.actions.contractors.RegistrationAddClientSite">
			<result>struts/contractors/registrationAddClientSite.jsp</result>
			<result name="input">struts/contractors/registrationAddClientSite.jsp</result>
			<result name="ClientSiteList">struts/contractors/registrationAddClientSiteListAjax.jsp</result>
			<result name="GeneralContractorSiteList">struts/contractors/_registrationAddClientSiteModal.jsp</result>
		</action>
		
		<action name="RegistrationServiceEvaluation"
			class="com.picsauditing.actions.contractors.RegistrationServiceEvaluation">
			<result>struts/contractors/registrationServiceEvaluation.jsp</result>
			<result name="input">struts/contractors/registrationServiceEvaluation.jsp</result>
		</action>

		<action name="RegistrationMakePayment"
			class="com.picsauditing.actions.contractors.RegistrationMakePayment">
			<result>struts/contractors/registrationMakePayment.jsp</result>
			<result name="print">struts/contractors/registrationMakePaymentPrint.jsp</result>
			<result name="blank">/struts/blank.jsp</result>
		</action>

        <action name="MockBrainTreeTransact" class="com.picsauditing.actions.billing.MockBrainTreeTransact">
            <result>struts/blank.jsp</result>
            <result name="input">struts/blank.jsp</result>
        </action>

    </package>
</struts>