<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="com.picsauditing.actions.rest.api" extends="com.picsauditing">

		<action name="DataFeed" class="com.picsauditing.actions.rest.api.DataFeed">
		</action>

        <action name="ReportData" class="com.picsauditing.actions.rest.api.DataFeed">
        </action>

		<action name="ApiCheck" class="com.picsauditing.actions.rest.api.ApiCheck">
		</action>

        <action name="SeleniumKey" class="com.picsauditing.selenium.SeleniumKey">
        </action>

        <!-- Auth Layer -->
        <action name="AuthService" class="com.picsauditing.authentication.service.AuthService">
        </action>

		<!-- Exceptions -->
		<action name="ReportExceptions" class="com.picsauditing.actions.report.ReportExceptions">
			<result>struts/reports/report_exceptions.jsp</result>
