<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="com.picsauditing.notes" extends="com.picsauditing">
		<action name="OperatorNotes" class="com.picsauditing.actions.notes.OperatorNotes">
			<result>struts/notes/account_notes.jsp</result>
		</action>
		
		<action name="ContractorNotes" class="com.picsauditing.actions.notes.ContractorNotes">
			<result>struts/notes/account_notes.jsp</result>
		</action>
		
		<action name="OperatorNotesAjax" class="com.picsauditing.actions.notes.OperatorNotes">
			<result name="notes">struts/notes/account_notes_notes.jsp</result>
		</action>
		
		<action name="CorporateNotesAjax" class="com.picsauditing.actions.notes.OperatorNotes">
			<result name="notes">struts/notes/account_notes_notes.jsp</result>
		</action>
		
		<action name="ContractorNotesAjax" class="com.picsauditing.actions.notes.ContractorNotes">
			<result name="notes">struts/notes/account_notes_notes.jsp</result>
		</action>

		<action name="EmbeddedNotesAjax" class="com.picsauditing.actions.AccountActionSupport">
			<result>struts/notes/account_notes_embed.jsp</result>
		</action>
		
		<action name="NoteEditor" class="com.picsauditing.actions.notes.NoteEditor">
			<result name="edit">struts/notes/account_notes_edit.jsp</result>
			<result name="view">struts/notes/account_notes_view.jsp</result>
		</action>
	</package>
</struts>