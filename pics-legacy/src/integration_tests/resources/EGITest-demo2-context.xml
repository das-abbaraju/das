  <bean id="EmployeeSiteStatusProcess" class="com.picsauditing.employeeguard.process.EmployeeSiteStatusProcess"/>
  <bean id="EmployeeSkillDataProcess" class="com.picsauditing.employeeguard.process.EmployeeSkillDataProcess"/>
  <bean id="ProfileSkillStatusProcess" class="com.picsauditing.employeeguard.process.ProfileSkillStatusProcess"/>
  <bean id="ProcessHelper" class="com.picsauditing.employeeguard.process.ProcessHelper"/>

  <!-- Factories -->
  <bean id="CompanySkillsFormBuilder" class="com.picsauditing.employeeguard.forms.factory.CompanySkillsFormBuilder"/>
  <bean id="EmployeeProfileEditFormBuilder"
        class="com.picsauditing.employeeguard.forms.factory.EmployeeProfileEditFormBuilder"/>
  <bean id="EmployeeProfileFormBuilder"
        class="com.picsauditing.employeeguard.forms.factory.EmployeeProfileFormBuilder"/>
  <bean id="FormBuilderFactory" class="com.picsauditing.employeeguard.forms.factory.FormBuilderFactory"/>
  <bean id="ProjectInfoFactory" class="com.picsauditing.employeeguard.forms.factory.ProjectInfoFactory"/>

  <!-- Utilities -->
  <bean id="PhotoUtil" class="com.picsauditing.employeeguard.util.PhotoUtil" scope="prototype"/>

  <!-- from picsorg -->
  <bean id="AppPropertyService" class="com.picsauditing.service.AppPropertyService"/>
  <bean id="FileSystemAccessor" class="com.picsauditing.util.FileSystemAccessor"/>
  <bean id="AppPropertyDAO" class="com.picsauditing.dao.AppPropertyDAO"/>
  <!--<bean id="AccountDAO" class="com.picsauditing.dao.SpringConfiguredSlickEnhancedAccountDAO"/>-->
  <bean id="AccountDAO" class="com.picsauditing.dao.AccountDAO"/>
