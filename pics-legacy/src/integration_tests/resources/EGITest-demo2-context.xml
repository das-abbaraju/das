  <bean id="EmployeeDAO" class="com.picsauditing.employeeguard.daos.EmployeeDAO"/>
  <bean id="ProfileDAO" class="com.picsauditing.employeeguard.daos.ProfileDAO"/>
  <bean id="ProfileDocumentDAO" class="com.picsauditing.employeeguard.daos.ProfileDocumentDAO"/>
