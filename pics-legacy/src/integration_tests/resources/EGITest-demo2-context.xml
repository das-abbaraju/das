  <bean id="ProjectSkillDAO" class="com.picsauditing.employeeguard.daos.ProjectSkillDAO"/>
  <bean id="ProjectSkillRoleDAO" class="com.picsauditing.employeeguard.daos.ProjectSkillRoleDAO"/>
  <bean id="RoleDAO" class="com.picsauditing.employeeguard.daos.RoleDAO"/>
  <bean id="SiteAssignmentDAO" class="com.picsauditing.employeeguard.daos.SiteAssignmentDAO"/>
  <bean id="SiteSkillDAO" class="com.picsauditing.employeeguard.daos.SiteSkillDAO"/>
  <bean id="SkillCalculatorService" class="com.picsauditing.employeeguard.services.status.StatusCalculatorService"/>
  <bean id="SoftDeletedEmployeeDAO" class="com.picsauditing.employeeguard.daos.softdeleted.SoftDeletedEmployeeDAO"/>
  <bean id="PicsorgDAO" class="com.picsauditing.employeeguard.daos.PicsorgDAO"/>


  <!-- Services -->
  <bean id="AccountGroupEmployeeService" class="com.picsauditing.employeeguard.services.AccountGroupEmployeeService"/>
  <bean id="AccountSkillProfileService" class="com.picsauditing.employeeguard.services.AccountSkillProfileService"/>
  <bean id="AppUserService" class="com.picsauditing.authentication.service.AppUserService"/>
  <bean id="ContractorProjectService" class="com.picsauditing.employeeguard.services.ContractorProjectService"/>
  <bean id="EmailHashService" class="com.picsauditing.employeeguard.services.email.EmailHashService"/>
  <bean id="EmailService" class="com.picsauditing.employeeguard.services.email.EmailService"/>
  <bean id="EmployeeService" class="com.picsauditing.employeeguard.services.EmployeeService"/>
