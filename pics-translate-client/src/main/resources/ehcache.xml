<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<diskStore path="java.io.tmpdir" />

	<!--
		These settings will only be applied to a cache created
		programmatically via CacheManager.add(String cacheName)
	-->
	<cache name="i18n" maxElementsInMemory="20000" eternal="false"
		timeToLiveSeconds="3600" timeToIdleSeconds="1800" overflowToDisk="false"
		memoryStoreEvictionPolicy="LRU">
	</cache>

    <cache name="i18n-wildcards" maxElementsInMemory="20000" eternal="false"
           timeToLiveSeconds="3600" timeToIdleSeconds="1800" overflowToDisk="false"
           memoryStoreEvictionPolicy="LRU">
    </cache>

</ehcache>
