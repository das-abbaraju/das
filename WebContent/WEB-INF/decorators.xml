<decorators defaultdir="/decorators">
	<decorator name="marketing" page="marketing.jsp">
		<pattern>/index.jsp</pattern>
		<pattern>/clients.jsp</pattern>
		<pattern>/contact.jsp</pattern>
		<pattern>/confirmContact.jsp</pattern>
		<pattern>/career_opportunity.jsp</pattern>
		<pattern>/services.jsp</pattern>
		<pattern>/login.jsp</pattern>
		<pattern>/forgot_password.jsp</pattern>
		<pattern>/contractor_new.jsp</pattern>
		<pattern>/contractor_new_instructions.jsp</pattern>
